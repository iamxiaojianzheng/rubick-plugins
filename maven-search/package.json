{
  "name": "@jiahe/rubick-maven-search",
  "author": "JIAHE",
  "license": "MIT",
  "version": "1.0.2",
  "pluginType": "ui",
  "preload": "preload.js",
  "pluginName": "Maven搜索",
  "logo": "https://pica.zhimg.com/v2-b39ef280661e794671f49a844e7276aa.png",
  "homePage": "https://raw.githubusercontent.com/iamxiaojianzheng/rubick-plugins/refs/heads/main/maven-search/README.md",
  "description": "Rubick插件：最快捷的Maven搜索。自研搜索算法，无论单词多长，输入你记得的前几个字母即可。热度排序，使用的人数越多，结果越智能。",
  "scripts": {
    "build": "sh build.sh",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamxiaojianzheng/rubick-plugins/tree/main/maven-search"
  },
  "features": [
    {
      "code": "maven",
      "explain": "输入内容查询依赖，复制格式为maven格式",
      "cmds": [
        "maven",
        "mvn",
        {
          "type": "over",
          "label": "maven"
        }
      ]
    },
    {
      "code": "gradle",
      "explain": "输入内容查询依赖，复制格式为gradle格式",
      "cmds": [
        "gradle",
        {
          "type": "over",
          "label": "maven"
        }
      ]
    },
    {
      "code": "kotlin",
      "explain": "输入内容查询依赖，复制格式为gradle(kotlin)格式",
      "cmds": [
        "kotlin",
        {
          "type": "over",
          "label": "maven"
        }
      ]
    }
  ]
}
