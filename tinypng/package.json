{
  "name": "@jiahe/rubick-tinypng",
  "author": "JIAHE",
  "version": "1.0.1",
  "pluginType": "ui",
  "main": "index.html",
  "preload": "preload.js",
  "pluginName": "tinypng压缩图片",
  "description": "使用tinypng压缩图片",
  "homePage": "https://raw.githubusercontent.com/iamxiaojianzheng/rubick-plugins/refs/heads/main/tinypng/README.md",
  "logo": "https://picx.zhimg.com/v2-b642bdc9473b57d609f9bdf367a3dc42.png",
  "features": [
    {
      "code": "tinypng",
      "explain": "压缩图片",
      "cmds": [
        "tinypng",
        "压缩图片",
        {
          "type": "files",
          "fileType": "file",
          "match": "/\\.(png|jpeg|jpg|webp)$/i",
          "label": "压缩图片"
        },
        {
          "type": "files",
          "fileType": "directory",
          "label": "压缩此文件夹下所有图片"
        },
        {
          "type": "window",
          "match": {
            "app": [
              "Finder.app",
              "explorer.exe",
              "SearchApp.exe"
            ],
            "class": [
              "CabinetWClass",
              "ExploreWClass"
            ]
          },
          "label": "压缩此文件夹下所有图片"
        }
      ]
    }
  ]
}
